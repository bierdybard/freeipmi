#! @prefix@/sbin/fish -s
!#
(catch 'misc-error
       (lambda ()
	 (let ((cmd-args (bmc-config-argp (fi-command-line))))
	   (if (list? cmd-args)
	       (bmc-config-main cmd-args))))
       (lambda (k args . opts)
	 (display "bmc-config: error: ")
	 (display (cadr opts))
	 (newline)))
(fi-exit bmc-config-exit-status)

